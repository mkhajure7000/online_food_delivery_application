<h1>Login</h1>
<div class="w-25 container">
  <%= form_with url: sessions_path  do |form| %>
    <div class="mb-3">
      <%= label_tag 'Email' %><br>
      <%= text_field_tag :email , params[:email] %>
    </div>
    <div class="mb-3">
      <%= label_tag :password %><br>
      <%= password_field_tag :password %><br>
      <%= link_to "Forgot your password?", password_path %>
    </div>
    <div>
      <%= submit_tag "Log in "%>
    </div>
 <%end %>
</div>
