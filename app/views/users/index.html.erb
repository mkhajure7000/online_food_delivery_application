<% if current_user.present? %>
  <h1>Welcome user</h1>
  <h1><%= button_to "logout", {:controller => :sessions, :action => 'destroy', :id => current_user.id }, :method => :delete, class: "btn btn-success"%></h1>
<% else %>
  <h1>
    Welcome<br>
    <%= link_to "Sign up", new_user_path, class: "btn btn-success" %><br>
    <%= link_to "login", new_session_path, class: "btn btn-success" %>
  </h1>
<%end%>
